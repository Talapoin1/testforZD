{
  "description": "Create Invoice PDF and/or export to a cloud directory",
  "image": "https://raw.githubusercontent.com/zippydoc/Pictograms/master/PDF-Writer.gif",
  "scriptSettings": {
    "autoLoadTable": [
      "table1",
      "table2"
    ],
    "countries": [
      "US",
      "UK"
    ],
    "languages": [
      "EN"
    ],
    "displayName": "Invoice PDF",
    "payingUsersOnly": true,
    "explanationPicture": "",
