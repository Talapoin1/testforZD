{
  "description": "通过匹配标题和标签信息使用来源表格的内容自动填充目标表格",
  "image": "https://raw.githubusercontent.com/zippydoc/Pictograms/master/Autofill-1.gif",
  "scriptSettings": {
    "autoLoadTable": [
    ],
    "countries": [
      "US",
      "UK"
    ],
    "languages": [
      "CN"
    ],
    "displayName": "自动填充",
    "payingUsersOnly": true,
    "type": "ExpertTableTask"
  },
  "menu": [
    {
      "source": "https://api.github.com/repos/zippydoc/menue-snippets/contents/CN/AutofillInput.json?ref=main"
    },
    {
      "source": "https://api.github.com/repos/zippydoc/menue-snippets/contents/CN/AutofillOptions.json?ref=main"
    },
    {
      "source": "https://api.github.com/repos/zippydoc/menue-snippets/contents/common-snippets/CN/StandardOutput.json?ref=main",
      "children": [
        {
          "source": "https://api.github.com/repos/zippydoc/menue-snippets/contents/common-snippets/CN/OutputConfig.json?ref=main",
          "property": "output/fields",
          "after": "output.custom_options"
        },
        {
          "source": "https://api.github.com/repos/zippydoc/menue-snippets/contents/common-snippets/CN/DownloadSettings.json?ref=main",
          "property": "output/fields",
          "after": "output_config_frame"
        },
        {
          "source": "https://api.github.com/repos/zippydoc/menue-snippets/contents/common-snippets/CN/SelectOutputColumn.json?ref=main",
          "property": "output/fields",
          "after": "download_setting_frame"
        },
        {
          "source": "https://api.github.com/repos/zippydoc/menue-snippets/contents/common-snippets/CN/IfTableExist.json?ref=main",
          "property": "output/fields/output_config_frame/fields",
          "after": "output.tag_name"
        }
      ]
    }
  ],
  "script": "https://api.github.com/repos/zippydoc/basic/contents/Autofill.py?ref=master",
  "version": 1.0
}