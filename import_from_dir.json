{
  "description": "Import data from a cloud directory",
  "image": "https://raw.githubusercontent.com/zippydoc/Pictograms/master/ImportFromDirectory.gif",
  "scriptSettings": {
    "autoLoadTable": [
    ],
    "countries": [
      "US",
      "UK"
    ],
    "languages": [
      "EN"
    ],
    "displayName": "Import Directory",
    "payingUsersOnly": true,
    "explanationPicture": "",
    "type": "Beta",
	"attributes": [
      "0-input","Guest-User","Starter-Screen"
	],
    "ms_oauth2_scope": "files.read.all",
    "ms_oauth2_menu_condition": "options.webservice=='ms_one_drive'"
  },
  "menu": [
    {
      "source": "https://api.github.com/repos/zippydoc/menue-snippets/contents/ImportFromDirOptions.json?ref=prod"
    },
    {
      "source": "https://api.github.com/repos/zippydoc/menue-snippets/contents/ImportFromDirOutput.json?ref=prod",
      "children": [
        {
          "source": "https://api.github.com/repos/zippydoc/menue-snippets/contents/common-snippets/OutputConfig.json?ref=prod",
          "property": "output/fields",
          "after": "output.custom_options"
        },
        {
          "source": "https://api.github.com/repos/zippydoc/menue-snippets/contents/common-snippets/DownloadSettings.json?ref=prod",
          "property": "output/fields",
          "after": "output_config_frame"
        },
        {
          "source": "https://api.github.com/repos/zippydoc/menue-snippets/contents/common-snippets/IfTableExist.json?ref=prod",
          "property": "output/fields/output_config_frame/fields",
          "after": "output.tag_name"
        }
      ]
    }
  ],
  "script": "https://api.github.com/repos/zippydoc/basic/contents/Import.py?ref=prod",
  "version": 1.0
}
