      "US",
      "UK"
    ],
    "languages": [
      "FR"
    ],
    "displayName": "Remplissage auto",
    "payingUsersOnly": true,
    "type": "ExpertTableTask"
  },
  "menu": [
    {
      "source": "https://api.github.com/repos/zippydoc/menue-snippets/contents/FR/AutofillInput.json?ref=main"
    },
    {
      "source": "https://api.github.com/repos/zippydoc/menue-snippets/contents/FR/AutofillOptions.json?ref=main"
    },
    {
      "source": "https://api.github.com/repos/zippydoc/menue-snippets/contents/common-snippets/FR/StandardOutput.json?ref=main",
      "children": [
        {
          "source": "https://api.github.com/repos/zippydoc/menue-snippets/contents/common-snippets/FR/OutputConfig.json?ref=main",
          "property": "output/fields",
          "after": "output.custom_options"
        },
        {
          "source": "https://api.github.com/repos/zippydoc/menue-snippets/contents/common-snippets/FR/DownloadSettings.json?ref=main",
          "property": "output/fields",
          "after": "output_config_frame"
        },
        {
          "source": "https://api.github.com/repos/zippydoc/menue-snippets/contents/common-snippets/FR/SelectOutputColumn.json?ref=main",
          "property": "output/fields",
          "after": "download_setting_frame"
        },
        {
          "source": "https://api.github.com/repos/zippydoc/menue-snippets/contents/common-snippets/FR/IfTableExist.json?ref=main",
          "property": "output/fields/output_config_frame/fields",
          "after": "output.tag_name"
        }
      ]
    }
  ],
